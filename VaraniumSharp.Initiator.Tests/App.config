<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="log.console" value="true" />
    <add key="log.file" value="true" />
    <add key="log.file.path" value="log.txt" />
    <add key="log.seq" value="true" />
    <add key="seq.host" value="https://seqhost.com" />
    <add key="log.splunk" value="true" />
    <add key="log.splunk.host" value="https://mysplunk:8088/services/collector" />
    <add key="log.splunk.token" value="ReplaceWithToken" />
  </appSettings>

  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Castle.Core" publicKeyToken="407dd0808d44fbdc" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-999.999.999.999" newVersion="4.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>